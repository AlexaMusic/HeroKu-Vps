      "port": "80",
      "healthcheck": {
        "command": "curl --fail http://localhost/ || exit 1",
        "interval": 30,
        "timeout": 10,
        "threshold": 1,
        "restart": {
          "limit": 3,
          "interval": 60,
          "type": "immediate"
        }
      },
      "routes": [
        {
          "hostname": "my-app.herokuapp.com"
        }
      ],
      "command": "/1.sh"
    }
  },
  "processes": {
    "web": "1"
  },
  "ports": [
    "80",
    "8888",
    "8080",
