{
  "name": "ngrok-ssh",
  "description": "A Docker container for SSH tunneling with ngrok",
  "image": "your-docker-image-name",
  "addons": [
    "heroku-postgresql"
  ],
  "buildpacks": [
    {
      "url": "heroku/docker"
    }
  ],
  "environments": {
    "production": {
      "ngrokid": {
        "description": "Ngrok Auth Token",
        "required": true
      },
      "Password": {
        "description": "SSH Password",
        "required": true
      }
    }
  },
  "scripts": {
    "postdeploy": "/1.sh"
  },
  "formation": {
    "web": {
      "quantity": 1,
      "size": "Free"
    }
  },
  "stack": "container"
}
